generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model sensor_data {
  time                DateTime @id(map: "sensor_data_pk") @unique(map: "sensor_data_time_idx") @default(now()) @db.Timestamptz(6)
  temperature         Float    @db.Real
  humidity            Float    @db.Real
  barometric_pressure Float    @db.Real
  gas                 Float    @db.Real
}
